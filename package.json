{
  "name": "@jupyterlab/google-drive",
  "version": "0.1.1",
  "description": "Realtime collaboration with JupyterLab through Google Drive",
  "author": "Ian Rose",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "google drive"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.6.0",
    "@jupyterlab/apputils": "^0.6.0",
    "@jupyterlab/coreutils": "^0.6.0",
    "@jupyterlab/docmanager": "^0.6.0",
    "@jupyterlab/docregistry": "^0.6.0",
    "@jupyterlab/filebrowser": "^0.6.0",
    "@jupyterlab/services": "^0.45.0",
    "@phosphor/application": "^1.2.0",
    "@phosphor/disposable": "^1.1.0",
    "@phosphor/commands": "^1.2.0",
    "@phosphor/algorithm": "^1.1.0",
    "@phosphor/coreutils": "^1.1.0",
    "@phosphor/signaling": "^1.2.0",
    "@phosphor/widgets": "^1.2.0",
    "es6-promise": "^4.1.0",
    "jquery": "^2.2.0",
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "typescript": "^2.2.1",
    "@types/google-drive-realtime-api": "^0.0.33",
    "@types/jquery": "^2.0.32"
  }
}
